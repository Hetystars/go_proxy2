{

  "Servers": [
    {
      "Tls": {
        "On": true,
        "Tcp_encrypt":true,
        "Server_cert_path": "cert/serv/server.crt",
        "Server_private_key_path": "cert/serv/server.key",
        "Client_cert_paths": [
          "cert/client/client.crt"
        ]
      },
      "Listen_port": 9999,
      "Enc_method": "chacha20",
      "Password": "12345678901234567890123456789012"
    }
  ],

  "Ulimit": 1024000,
  "Udp_timeout": 30
}
